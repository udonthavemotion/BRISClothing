{
  "functions": {
    "src/pages/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "routes": [
    {
      "src": "/success",
      "dest": "/success/index.html"
    },
    {
      "src": "/cancel",
      "dest": "/cancel/index.html"
    }
  ]
}
