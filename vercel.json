{
  "functions": {
    "src/pages/api/**/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  },
  "rewrites": [
    {
      "source": "/success",
      "destination": "/success/"
    },
    {
      "source": "/cancel", 
      "destination": "/cancel/"
    }
  ]
}
